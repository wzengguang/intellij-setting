<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Favorites">
        <Item>
          <IdeaActionID>ActivateFavoritesToolWindow</IdeaActionID>
          <ShortCut>'Alt+2'</ShortCut>
          <Description>Favorites</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Settings...">
        <Item>
          <IdeaActionID>ShowSettings</IdeaActionID>
          <ShortCut>'Alt+F7'</ShortCut>
          <Description>Settings...</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Structure">
        <Item>
          <IdeaActionID>ActivateStructureToolWindow</IdeaActionID>
          <ShortCut>'Alt+7'</ShortCut>
          <Description>Structure</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="TODO">
        <Item>
          <IdeaActionID>ActivateTODOToolWindow</IdeaActionID>
          <ShortCut>'Alt+6'</ShortCut>
          <Description>TODO</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Terminal">
        <Item>
          <IdeaActionID>ActivateTerminalToolWindow</IdeaActionID>
          <ShortCut>'Alt+F12'</ShortCut>
          <Description>Terminal</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Version Control">
        <Item>
          <IdeaActionID>ActivateVersionControlToolWindow</IdeaActionID>
          <ShortCut>'Alt+9' or 'Alt+Shift+9'</ShortCut>
          <Description>Version Control</Description>
          <Count>6</Count>
        </Item>
      </Statistic>
      <Statistic Action="Web">
        <Item>
          <IdeaActionID>ActivateWebToolWindow</IdeaActionID>
          <ShortCut>'Ctrl+Alt+W'</ShortCut>
          <Description>Web</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="编码规约扫描">
        <Item>
          <IdeaActionID>AliP3CInspectionAction</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Shift+J'</ShortCut>
          <Description>编码规约扫描</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>